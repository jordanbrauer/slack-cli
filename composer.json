    "scripts": {
      "test": "phpunit --configuration='./phpunit.xml' --test-suffix='.spec.php' --color=always ./dev/tests/"
    },
    "require": {
        "php": ">=7.0",
        "monolog/monolog": "^1.23",
        "symfony/console": "^3.3",
        "vlucas/phpdotenv": "^2.4",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
      "psr-0": {
        "Slack\\": "src/"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^6.2"
    },
    "autoload-dev": {
      "psr-4": {
        "Slack\\Tests\\": "dev/tests/"
      }
    }
}
